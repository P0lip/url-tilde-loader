{
  "extends": "airbnb-base",
  "env": {
    "jest": true
  },
  "rules": {
    "no-param-reassign": "warn"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js"
        ],
        "moduleDirectory": [
          "node_modules",
          "."
        ]
      }
    }
  }
}
